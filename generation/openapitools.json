{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.12.0",
    "generators": {
      "server": {
        "generatorName": "kotlin-spring",
        "output": "#{cwd}/example_output/coffee_shop",
        "inputSpec": "#{cwd}/input.yaml",
        "additionalProperties": {
          "apiTests": false
        }
      },
      "client": {
        "generatorName": "typescript-fetch",
        "output": "#{cwd}/example_output/coffee_shop_client",
        "inputSpec": "#{cwd}/input.yaml"
      }
    }
  }
}
